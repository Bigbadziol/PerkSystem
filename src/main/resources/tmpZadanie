public class ZadanieGlembokieRany extends BukkitRunnable {
    private PerkSystem plugin;
    private PerkGlembokieRany mojeZadanie;

    public ZadanieGlembokieRany(PerkSystem plugin) {
        this.plugin = plugin;
        mojeZadanie = (PerkGlembokieRany)plugin.perkLista.wezPerk("glembokieciecie");
    }

    @Override
    public void run() {
        int ile = mojeZadanie.ranni.size();
        System.out.println("Liczba rannych graczy : "+ile);
    }
}