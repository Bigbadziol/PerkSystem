public class PerkGlebokieRanyData {
    public final int czasTrwania = 15; // czas trwania efektu wykrwawiania, czas wyrazony w sekundach
    public final int iloscTickow = 3; // Ilosc obrazen zadana w okresalonym okresie czasu
    public final double procetObrazen = 0.3D; // Ilość procetowa z bazowych obrazen
    public int auktualnyTick = 0; // Auktualny tick na serverze
    public long czasWystartowaniaPerka = 0l; // Moment kiedy wystrtował perk na graczu
    public double bazoweObrazenie = 0D; //Bazowe obrazenie od ktorego naliczany zostanie pracent
    public double tickObrazenie = 0D; // obrazenia na ticka
    public long tickCzas = 0L;  // co ile ms ma byc naliczone obrazenie
    public long ostatniTickCzas = 0L; // ???????????????????????????????????

    public  PerkGlebokieRanyData(double cios, long czasSystemowy){
        bazoweObrazenie = cios;
        tickObrazenie = (cios * procetObrazen) / iloscTickow;
        tickCzas = (czasTrwania * 1000) / iloscTickow;
        czasWystartowaniaPerka = czasSystemowy;
    }